#include <iostream>
using namespace std;

int diziToplami(int dizi[], int boyut) {
    int toplam = 0;
    for (int i = 0; i < boyut; i++) {
        toplam += dizi[i];
    }
    return toplam;
}

int main() {
    int sayilar[] = {345,23,67,9,78,99};
    int boyut = 6;

    cout << "Toplam: " << diziToplami(sayilar, boyut) << endl;
    return 0;
}
